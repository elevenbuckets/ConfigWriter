{
  "name": "ConfigWriter",
  "version": "0.1.0",
  "description": "",
  "main": "main.js",
  "directories": {
    "test": "test"
  },
  "babel": {
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "8.6"
          }
        }
      ]
    ]
